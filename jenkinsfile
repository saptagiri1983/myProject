pipeline{
  agent any
  environment{
       PATH="/usr/bin/mvn:$PATH"
             }
     stages{
 	stage("checkout"){
 	  steps{
	      git 'https://github.com/saptagiri1983/myProject.git'
		}
			}
       stage("Build"){
	  steps{
             sh "mvn clean compile"
		}
	     }
       stage("test"){
          steps{
  	    sh "mvn test"
		}
       stage("package"){
          steps{
	    sh "mvn package"
		}

	  }
    }
}	
}
