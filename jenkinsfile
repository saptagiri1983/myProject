pipeline {
  agent any
  tools {
   maven  'M3'
        }
     stages {
 	stage('checkout') {
 	  steps {
	      git 'https://github.com/saptagiri1983/myProject.git'
		}
				}
       stage('Build') {
	  steps {
             sh 'mvn clean compile'
		}
	     }
	  }
       stage('Test') {
     	  steps {
             sh 'mvn test'
                }
                     }
      stage('package') {
         steps {
             sh 'mvn package'
	       }	
      }
    }
	
